{
    "configurations": [
        {
            "name": "Run FastAPI",
            "type": "python",
            "python": "${command:python.interpreterPath}",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}",
            "args": ["auth.main:app"]
        }, {
            "name": "Run tests",
            "type": "python",
            "python": "${command:python.interpreterPath}",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}",
            "args": ["${config:python.testing.pytestArgs}"],
        },
    ]
}